{% extends 'base.html.twig' %}

{% block title %}Hello contratController!{% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>
<table>
<tr>
<td>
<h1>Contrats</h1>
<ul>
{%for contrat in contrats%}
<li>{{contrat.dateDepart|date('Y-m-d') }} *{{contrat.dateRetour|date('Y-m-d')}} *{{contrat.kmDepart}} *{{contrat.kmRetour}}
{%endfor%}
</td>
</table>
{% endblock %}
